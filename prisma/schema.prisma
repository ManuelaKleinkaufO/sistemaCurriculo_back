generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Curriculo {
  id           String  @id @unique @default(uuid())
  nome         String
  email        String
  endereco_web String?
  experiencia  String
  telefone     String?
}
